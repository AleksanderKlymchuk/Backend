@model string
@using ViewModel.Infrastructure


@{
    ViewBag.Title = "index";
}



<h2></h2>
<table>
    <thead>
        <tr class="td">
            <a href="~/Views/Home/Contact.cshtml">~/Views/Home/Contact.cshtml</a>
            <td>Specie</td>
            <td>Price</td>
        </tr>
        
        @foreach (KeyValuePair<string,decimal> item in @ViewBag.pricelist)
        {
         <tr>
             <td>
                 @item.Key
             </td>
             <td>
               @string.Format("{0:.00}",  @item.Value)
             </td>
         </tr>



        }

    </thead>




</table>


<div class="div">
    @*invoking external helper*@
    @Html.DisplayDictionaryAsList((Dictionary<string, decimal>)@ViewBag.pricelist)

</div>

